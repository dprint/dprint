{
  "indentWidth": 2,
  "lineWidth": 160,
  "exec": {
    "associations": "**/*.rs",
    "rustfmt": "rustfmt --edition 2021 --config imports_granularity=item"
  },
  "excludes": [
    "website/playground/build",
    "website/_site",
    "**/node_modules",
    "**/dist",
    "**/target",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.85.1.wasm",
    "https://plugins.dprint.dev/json-0.17.4.wasm",
    "https://plugins.dprint.dev/markdown-0.15.3.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/prettier-0.26.1.json@fdbe31f6aecd24f9d6b924214710a6766050d03146163b4e241e6056b2462f2e",
    "https://plugins.dprint.dev/exec-0.3.5.json@d687dda57be0fe9a0088ccdaefa5147649ff24127d8b3ea227536c68ee7abeab"
  ]
}
