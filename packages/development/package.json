    "@dprint/types": "~0.2.0",
    "fast-glob": "^3.2.2"
  },
  "peerDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
